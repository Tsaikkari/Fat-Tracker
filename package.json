    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "google-auth-library": "^7.11.0",
    "jest": "^27.4.5",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^8.1.0",
    "mongoose": "^6.1.4",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "winston": "^3.3.3"
